import urllib
import urllib.request
import html.parser
import requests
from requests.exceptions import HTTPError
from socket import error as SocketError
from http.cookiejar import CookieJar

try:
    req=urllib.request.Request("http://www.mineduc.gob.gt/digeduca/", None, {'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/79.0.3945.130 Safari/537.36'
    ,'Accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.9',
    'Accept-Encoding': 'gzip, deflate','Accept-Language': 'en-US,en;q=0.9,es-419;q=0.8,es;q=0.7','Connection': 'keep-alive'})
    cj = CookieJar()
    opener = urllib.request.build_opener(urllib.request.HTTPCookieProcessor(cj))
    response = opener.open(req)
    raw_response = response.read().decode('utf8', errors='ignore')
    response.close()
    
    outfilename = "db_establecimientos_graduandos2019.xls"
    url_of_file = "http://www.mineduc.gob.gt/digeduca/apps/Bases_de_Datos_Evaluaciones/navegador/2019/documents/db_establecimientos_graduandos2019.xlsx"

    response = requests.get(url_of_file, cookies = cj)
    #urllib.request.urlretrieve(url_of_file, outfilename) 

    #print(response.text)

    open (outfilename , "wb" ).write(response.content)

except urllib.request.HTTPError as inst:
    output = format(inst)
    print(output)
